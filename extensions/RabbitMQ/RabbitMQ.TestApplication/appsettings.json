{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    }
  },
  "KernelMemory": {
    "Services": {
      "RabbitMQ": {
        "Host": "127.0.0.1",
        "Port": "5672",
        "Username": "user",
        "Password": "password",
        "VirtualHost": "/",
        "MessageTTLSecs": 3600,
        "SslEnabled": false,
        // How many times to dequeue a messages and process before moving it to a poison queue
        "MaxRetriesBeforePoisonQueue": 5,
        // Suffix used for the poison queues.
        "PoisonQueueSuffix": "-poison"
      }
    }
  }
}