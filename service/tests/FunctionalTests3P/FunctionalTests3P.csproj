<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <ManagePackageVersionsCentrally>false</ManagePackageVersionsCentrally>
        <TargetFramework>net7.0</TargetFramework>
        <IsTestProject>true</IsTestProject>
        <AssemblyName>Microsoft.FunctionalTests3P</AssemblyName>
        <ImplicitUsings>enable</ImplicitUsings>
        <GenerateDocumentationFile>false</GenerateDocumentationFile>
        <UserSecretsId>5ee045b0-aea3-4f08-8d31-32d1a6f8fed0</UserSecretsId>
        <NoWarn>$(NoWarn);IDE1006,CA1303,CA1307,CA1859,CA2007,CA2201,CS1591,CA1861,SKEXP0001</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
        <PackageReference Include="Microsoft.KernelMemory.Abstractions" Version="0.23.231224.1" />
        <PackageReference Include="Microsoft.KernelMemory.Core" Version="0.23.231224.1" />
        <PackageReference Include="Microsoft.KernelMemory.Postgres" Version="0.3.231224.1" />
        <PackageReference Include="Microsoft.KernelMemory.SemanticKernelPlugin" Version="0.23.231224.1" />
        <PackageReference Include="Microsoft.KernelMemory.WebClient" Version="0.23.231224.1" />
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.8.0" />
        <PackageReference Include="xunit" Version="2.6.4" />
        <PackageReference Include="xunit.abstractions" Version="2.0.3" />
        <PackageReference Include="Xunit.DependencyInjection" Version="8.9.1" />
        <PackageReference Include="Xunit.DependencyInjection.Logging" Version="8.1.0" />
        <PackageReference Include="xunit.runner.visualstudio" Version="2.5.6">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>

    <ItemGroup>
        <Content Include="file1-NASA-news.pdf">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </Content>
    </ItemGroup>

</Project>